{
  "version": 2,
  "buildCommand": "cd src/frontend && npm install && npm run build",
  "outputDirectory": "src/frontend/dist",
  "installCommand": "cd src/frontend && npm install",
  "framework": "vite",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "src/frontend/dist/**": {
      "includeFiles": "src/frontend/dist/**"
    }
  }
}